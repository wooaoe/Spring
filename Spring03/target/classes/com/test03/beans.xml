<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- kang이라는 이름을 가진 bean을 만들자 -->
	<!-- 강호동, 2000000, 기술 1팀 -->
	
	<!-- you라는 bean -->
	<!-- 유재석, 3000000, 개발 1팀 -->
	
	<!-- MTest에서 kang, you를 출력 -->
	
	<bean id = "kang" class = "com.test03.Engineer">
		<constructor-arg value = "강호동"></constructor-arg>
		<constructor-arg value = "2000000"></constructor-arg>
		<property name="dept" value = "기술 1팀"></property>
	</bean>
	
	<bean id = "you" class = "com.test03.Developer">
		<constructor-arg value = "유재석"></constructor-arg>
		<constructor-arg value = "3000000"></constructor-arg>
		<property name="dept" value = "개발 1팀"></property>
	</bean>

</beans>
